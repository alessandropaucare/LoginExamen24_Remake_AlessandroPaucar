<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/nav_graph"
    app:startDestination="@id/home">

    <fragment
        android:id="@+id/home"
        android:name="edu.iesam.loginexam1eval.features.user.presentation.HomeFragment"
        android:label="Home"
        >
        <action
            android:id="@+id/home_to_sign_up"
            app:destination="@+id/sign_up"
            />
    </fragment>
    <fragment
        android:id="@+id/sign_up"
        android:name="edu.iesam.loginexam1eval.features.user.presentation.SignUpFragment"
        android:label="SignUp"
        >
        <action
            android:id="@+id/sign_up_to_welome"
            app:destination="@+id/welcome"
            />
    </fragment>
    <fragment
        android:id="@+id/welcome"
        android:name="edu.iesam.loginexam1eval.features.user.presentation.WelcomeFragment"
        android:label="Welcome"
        >
    </fragment>
</navigation>